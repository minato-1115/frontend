{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "frontend/.expo/types/**/*.ts",
    "frontend/expo-env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
  ]
}
